<!DOCTYPE html>
<html lang="pt-br">

<head>
	<meta charset="utf-8" />
	<title>Notícias</title>
</head>

<body>

	<h1>Notícias</h1>

	<table>
		<tr>
			<td>ID</td>
			<td>TITULO</td>
		</tr>

		<% noticias.map(item=> { %><% //Javascript%>

			<tr>
				<td>
					<%= item.id_noticias %>
				</td>

				<td>
					<%= item.titulo %>
				</td>
			</tr>

		<% }) %>
	</table>

</body>

</html>